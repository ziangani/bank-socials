APP_NAME=Laravel
APP_FRIENDLY_NAME="Social Banking"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=social_banking
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# WhatsApp Configuration
WHATSAPP_ENABLED=true
WHATSAPP_TIMEOUT=300
WHATSAPP_RETRY_ATTEMPTS=3
WHATSAPP_WEBHOOK_URL=
WHATSAPP_VERIFY_TOKEN=
WHATSAPP_ACCESS_TOKEN=

# USSD Configuration
USSD_ENABLED=true
USSD_TIMEOUT=120
USSD_SERVICE_CODE=*123#
USSD_PROVIDER=default

# Transaction Limits
DEFAULT_CURRENCY=KES
MIN_TRANSACTION_AMOUNT=10
MAX_TRANSACTION_AMOUNT=150000

# Standard User Limits
STANDARD_DAILY_LIMIT=300000
STANDARD_SINGLE_LIMIT=100000
STANDARD_MONTHLY_LIMIT=1000000

# Premium User Limits
PREMIUM_DAILY_LIMIT=1000000
PREMIUM_SINGLE_LIMIT=500000
PREMIUM_MONTHLY_LIMIT=5000000

# Business User Limits
BUSINESS_DAILY_LIMIT=5000000
BUSINESS_SINGLE_LIMIT=1000000
BUSINESS_MONTHLY_LIMIT=20000000

# Transaction Fees
INTERNAL_FIXED_FEE=0
INTERNAL_PERCENTAGE_FEE=0
EXTERNAL_FIXED_FEE=50
EXTERNAL_PERCENTAGE_FEE=1
BILL_FIXED_FEE=30
BILL_PERCENTAGE_FEE=0

# Security Settings
MAX_PIN_ATTEMPTS=3
PIN_TIMEOUT=30
SESSION_TIMEOUT=30
OTP_EXPIRY=10
OTP_LENGTH=6

# Notifications
SMS_NOTIFICATIONS_ENABLED=true
SMS_PROVIDER=default
EMAIL_NOTIFICATIONS_ENABLED=true
MAIL_FROM_ADDRESS=
MAIL_FROM_NAME="${APP_FRIENDLY_NAME}"

# Core Banking Integration
CORE_BANKING_URL=
CORE_BANKING_TIMEOUT=30
CORE_BANKING_RETRY_ATTEMPTS=3

# Mobile Money Integration
MOBILE_MONEY_ENABLED=true
MOBILE_MONEY_PROVIDER=default
MOBILE_MONEY_URL=

# Bill Payments
BILL_PAYMENTS_ENABLED=true

# Logging
TRANSACTION_LOGGING_ENABLED=true
TRANSACTION_LOG_CHANNEL=daily
LOG_RETENTION_DAYS=90

# Simulator Settings
SIMULATOR_ENABLED=true
SIMULATOR_DELAY=2
SIMULATOR_ERROR_RATE=0.1

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

# AWS Configuration
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Pusher Configuration
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https
PUSHER_APP_CLUSTER=mt1

VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
